; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bullet Heck"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

UiManager="*res://UI_Manager.gd"
Tracker="*res://tracker.gd"
Explosion="res://explosion.gd"

[display]

window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/canvas_textures/default_texture_filter=1
window/snap/snap_2d_transforms_to_pixel=true

[global_group]

player=""
projectiles=""

[input]

grab={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"location":0,"echo":false,"script":null)
]
}

[layer_names]

2d_physics/layer_1="Entities"
2d_physics/layer_2="Bullets"
2d_physics/layer_3="Terrain"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.615686, 0.431373, 0.717647, 1)
